exports[`src/pages/Register IndihomeNumber 1`] = `
<WhiteCard
  back="/register/verify"
  icon="wifi"
  subtitle=""
  title="Apakah Anda sudah berlangganan layanan IndiHome?"
>
  <Card
    className=""
    disabled={false}
    onClick={[Function]}
    to=""
    variant="bordered"
  >
    <div>
      <span>
        Ya
      </span>
    </div>
    <div
      className=""
    >
      <label>
        Nomor Indihome
      </label>
      <input
        maxLength="12"
        onChange={[Function]}
        placeholder="Masukkan nomor Indihome Anda di sini"
        type="text"
      />
    </div>
  </Card>
  <Card
    className=""
    disabled={false}
    onClick={[Function]}
    to=""
    variant="bordered"
  >
    <div>
      <span>
        Tidak
      </span>
    </div>
  </Card>
  <Button
    className=""
    disabled={true}
    onClick={[Function]}
    to=""
    type="button"
    variant="red"
  >
    Lanjut
  </Button>
</WhiteCard>
`;

