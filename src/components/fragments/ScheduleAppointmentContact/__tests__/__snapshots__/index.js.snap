exports[`src/components/fragments/ScheduleAppointmentContact Add 1`] = `
<button
  onClick={[Function]}
>
  <img
    alt="address"
    src="/assets/ic_location_red.svg"
  />
  <span>
    <h4>
      Add Secondary Contact
    </h4>
    <p>
      Not home during the installation?
    </p>
    <p>
      Add a second contact person.
    </p>
  </span>
  <img
    alt=""
    src="/assets/ic_chevron_white.svg"
  />
</button>
`;

exports[`src/components/fragments/ScheduleAppointmentContact Edit 1`] = `
